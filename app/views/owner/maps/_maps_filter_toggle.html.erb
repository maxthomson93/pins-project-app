<div class="toggle-wrapper">
  <input
    type="checkbox"
    class="map-filter filter-map-toggle <%= color %>-toggle" data-action="google-maps#filterToggle"  data-google-maps-target="checkboxes" data-map-index="<%= i %>"
    id="map_<%=i%>"
    checked
  >
    <label for="map_<%=i%>"></label>
  </div>
